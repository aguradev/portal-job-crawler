{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "ES6",
        "jsx": "react",
        "strict": true,
        "allowJs": true,
        "paths": {
            "@/*": ["./src/*"],
            "@ui/*": ["./src/components/ui/*"],
            "@element/*": ["./src/components/element/*"],
        }
    },
    "include": [
        "src"
    ],
    "exclude": ["node_modules","**/node_modules", "dist", "public"],
}